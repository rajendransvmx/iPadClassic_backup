    ],

    "declares" : [

    ],

    "defines" : [
        {   "type" : "com.servicemax.client.runtime.application",
            "config" : {
                "application" : { "id" : "installigence", "class-name" : "com.servicemax.client.installigence.app.Application" }
            }
        },
        {
            "type" : "com.servicemax.client.runtime.namedinstance",
            "config" : {
