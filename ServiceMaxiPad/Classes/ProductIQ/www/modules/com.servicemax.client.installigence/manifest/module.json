{   "id" : "com.servicemax.client.installigence" ,
    "name" : "ServiceMax Installigence",
    "description" : "ServiceMax Installigence",
    "version" : "1.0.0",
    "module-activator" : "com.servicemax.client.installigence.impl.Module",

